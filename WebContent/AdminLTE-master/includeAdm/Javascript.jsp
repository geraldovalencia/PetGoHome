<script>
	function cliqueMenu() {
		
		var laisgomes;
		
		var e = document.getElementById("body");
		
		if (e == document.querySelector(".skin-blue")){
			
			if(window.outerWidth <= 800){
			document.getElementById("body").setAttribute("class",
			"skin-blue wysihtml5-supported sidebar-open");
				
			}else if(window.outerWidth >= 801){
				document.getElementById("body").setAttribute("class",
				"skin-blue wysihtml5-supported sidebar-collapse");
			}
			
			
			document.getElementById('body').id = 'novoID';
			document.getElementById('body').name = 'novoID';
			
		}
		
		var j = document.getElementById("novoID");
		
		
		if(j != null){
			//alert("Entrou no ife");
			document.getElementById("novoID").setAttribute("class", 
			"skin-blue");
			document.getElementById('novoID').id = 'body';
		}
		
		
			
	}
</script>